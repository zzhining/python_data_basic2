   "source": [
    "# try-except-finally\n",
    "\n",
    "```\n",
    "try:\n",
    "    # 예외가 발생할 수 있는 코드\n",
    "    pass\n",
    "except ExceptionType as e:\n",
    "    # 특정 예외 처리\n",
    "    pass\n",
    "except:\n",
    "    # 모든 예외 처리\n",
    "    pass\n",
    "finally:\n",
    "    # 항상 실행되는 코드\n",
    "    pass\n",
    "```"
   ]
  },
